<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.liuliu.xi.cityofanimation">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activity.FrameAnimationActivity"
            android:label="@string/main_activity_frame_animation" />
        <activity
            android:name=".activity.TweenActivity"
            android:label="@string/main_activity_tween_animation" />
        <activity
            android:name=".activity.BlurActivity"
            android:label="Blur" />
        <activity android:name=".activity.SwitchAnimActivity"></activity>
    </application>

</manifest>